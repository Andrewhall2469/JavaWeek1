import java.util.ArrayList;
import java.util.Scanner;

public class garage {
	static ArrayList<vehicle> vehicleArray = new ArrayList<vehicle>();
	
	
public static void calculateBill() {
	
	for (vehicle v : vehicleArray) {
		
		System.out.print("Cost of " + v.type + ": £");
		System.out.println(((vehicle) v).bill());
		System.out.println("---------------------------");
		
		}
	
}

public static void addVehicle() {
	Scanner input = new Scanner();
	System.out.println("Enter Vehicle ID you wish to be removed: ");
	vehicle = input.next();
}


}

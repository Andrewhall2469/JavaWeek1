
public class car extends vehicle {
	public String fuelGrade;
	public int numOfDoors;
	
	
	public car(int numberOfWheels, boolean motorised, String colour, double weight, String makeAndModel, String vinNo, 
			String fuelGrade, int numOfDoors) {
		super(numberOfWheels, motorised, colour, weight, makeAndModel, vinNo);
		this.fuelGrade = fuelGrade;
		this.numOfDoors = numOfDoors;
		
	}

}

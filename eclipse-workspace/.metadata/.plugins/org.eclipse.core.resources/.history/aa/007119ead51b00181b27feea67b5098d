
public class airplane extends vehicle {
	int numOfWings;
	int numOfSeats;

	public airplane(int numberOfWheels, boolean motorised, String colour, double weight, String makeAndModel,
			String vinNo, int numOfWings, int numOfSeats) {
		super(numberOfWheels, motorised, colour, weight, makeAndModel, vinNo);
		this.numOfWings = numOfWings;
		this.numOfSeats = numOfSeats;
 
	}

}

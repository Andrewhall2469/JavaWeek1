import java.util.ArrayList;

public class garage {
	static ArrayList<vehicle> vehicleArray = new ArrayList<vehicle>();
	
	
public static void calculateBill() {
	
	for (vehicle v : vehicleArray) {
		System.out.println("---------------------------");
		System.out.print("Cost of " + v.type + ": £");
		System.out.println(((vehicle) v).bill());
		
		}
	
}


}

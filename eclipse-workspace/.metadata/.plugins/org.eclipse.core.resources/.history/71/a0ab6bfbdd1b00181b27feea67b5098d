public class vehicle{
	int numberOfWheels;
	boolean motorised;
	String colour;
	String makeAndModel;
	String vinNo;
	
	public vehicle(int numberOfWheels, boolean motorised, String colour, String makeAndModel, String vinNo) {
		this.numberOfWheels = numberOfWheels;
		this.motorised = motorised;
		this.colour = colour;
		this.makeAndModel = makeAndModel;
		this.vinNo = vinNo;
	}
	
}


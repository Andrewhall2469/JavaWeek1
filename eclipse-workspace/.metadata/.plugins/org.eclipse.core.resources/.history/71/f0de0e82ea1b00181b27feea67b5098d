import java.util.ArrayList;

public class garage {
	ArrayList<vehicle> vehicleArray = new ArrayList<vehicle>();
	
	
public static int calculateBill(ArrayList<vehicle> vehicleArray) {
	
	for (Object v : vehicleArray) {
		if(v.toString().contains("car")) {
			System.out.println(v);
			return 2000;
		}
		if (v.toString().contains("motorcycle")) {
			System.out.println(v);
			return 1000;

		}
		if (v.toString().contains("airplane")) {
			System.out.println(v);
			return 30000;

		}
		}
	return 0;
	
}


}

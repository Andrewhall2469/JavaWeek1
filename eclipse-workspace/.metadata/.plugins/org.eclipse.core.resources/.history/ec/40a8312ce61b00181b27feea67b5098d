import java.util.ArrayList;

public class garage {
	ArrayList<vehicle> vehicleArray = new ArrayList<vehicle>();
	
	
public static int calculateBill(ArrayList<vehicle> vehicleArray) {
	
	for (Object vehicle : vehicleArray) {
		if(vehicle.toString().contains("motorcycle")) {
			return 2000;
		}
		else if (vehicle.toString().contains("motorcycle")) {
			return 1000;

		}
		else if (vehicle.toString().contains("airplane")) {
			return 30000;

		}
		}
	return 0;
	
}


}

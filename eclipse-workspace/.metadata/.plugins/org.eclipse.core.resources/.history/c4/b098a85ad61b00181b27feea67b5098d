
public class motorcycle extends vehicle {
	public String engineSize;
	public String tyreBrand;

	public motorcycle(int numberOfWheels, boolean motorised, String colour, int weight, String makeAndModel,
			String vinNo, String engineSize, String tyreBrand) {
		super(numberOfWheels, motorised, colour, weight, makeAndModel, vinNo);
		this.engineSize = engineSize;
		this.tyreBrand = tyreBrand;
		
	}
	public String toString() {
		return numberOfWheels + " " + motorised + " " + colour + " " + weight + " " + makeAndModel + " " + vinNo + " " 
				+ " " + engineSize + " " + tyreBrand;
	}

}

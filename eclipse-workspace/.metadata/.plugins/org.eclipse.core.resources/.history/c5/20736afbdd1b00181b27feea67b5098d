
public class airplane extends vehicle {
	int numOfWings;
	int numOfSeats;

	public airplane(int numberOfWheels, boolean motorised, String colour, String makeAndModel,
			String vinNo, int numOfWings, int numOfSeats) {
		super(numberOfWheels, motorised, colour, makeAndModel, vinNo);
		this.numOfWings = numOfWings;
		this.numOfSeats = numOfSeats;
 
	}
	
	public String toString() {
		return numberOfWheels + " " + motorised + " " + colour + " " + makeAndModel + " " + vinNo + " " 
				+ " " + numOfWings + " " + numOfSeats;
	}

}

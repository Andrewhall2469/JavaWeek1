
public class Factorials {

	public static void main(String[] args) {
	
//		int fact= 1;
		
		int number = 3628800;
		
		
		
		for (int i = 1; i <= number; i++)
		{	
			
			number = number/i;
			if (number == 1) {
				System.out.println(number + " = " + i + "!");
			
			}
			else if (number < 1)
			{
				System.out.println("none");
			
			}
//			System.out
		}
		
//		System.out.println("Factorial of "+number+" is: "+newNumber);    
		
		
	}
}

import java.util.ArrayList;

public class garage {
	ArrayList<vehicle> vehicleArray = new ArrayList<vehicle>();
	
	
public static void calculateBill(ArrayList<vehicle> vehicleArray) {
	
	for (vehicle v : vehicleArray) {
		System.out.print("Bill for" + v.type + "of id: " + v.id + "is £");
		System.out.println(((vehicle) v).bill());
		
		}
	
}


}
